<html>
<head>
  <meta charset='utf-8'>
  <title>REPL Project</title>
</head>
<body>
<h1>Queue Ouput</h1>
<table>
  <tr>
    <th>last_name</th>
    <th>first_name</th>
    <th>email</th>
    <th>zipcode</th>
    <th>city</th>
    <th>state</th>
    <th>address</th>
    <th>phone_number</th>
  </tr>
  <% data.each do |record| %>
    <tr>
      <td><%= "#{record[:last_name]}" %></td>
      <td><%= "#{record[:first_name]}" %></td>
      <td><%= "#{record[:email]}" %></td>
      <td><%= "#{record[:zipcode]}" %></td>
      <td><%= "#{record[:city]}" %></td>
      <td><%= "#{record[:state]}" %></td>
      <td><%= "#{record[:street]}" %></td>
      <td><%= "#{record[:phone]}" %></td>
    </tr>
    <% end %>
</table>
</body>
</html>
