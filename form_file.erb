<html>
<head>
  <meta charset='utf-8'>
  <title>REPL Project</title>
  <style>
    body {
      text-align: left;
      font-family: sans-serif;
      font-size: 12px;
    }
    table{
      border-collapse: collapse;
    }
    th, td{
      text-align: left;
      padding: 8px;
    }
    th{
      background-color: #4CAF50;
      color: white;
    }
    tr:nth-child(even){background-color: #f2f2f2}
  </style>
</head>
<body>
<h1>Queue Ouput</h1>
</br>
<table>
  <tr>
    <th>Last Name</th>
    <th>First Name</th>
    <th>Email</th>
    <th>Zipcode</th>
    <th>City</th>
    <th>State</th>
    <th>Address</th>
    <th>Phone Number</th>
    <th>District</th>
  </tr>
  <% data.each do |record| %>
    <tr>
      <td><%= "#{DataScrub::capitalize_name(record[:last_name])}" %></td>
      <td><%= "#{DataScrub::capitalize_name(record[:first_name].capitalize)}" %></td>
      <td><%= "#{record[:email]}" %></td>
      <td><%= "#{record[:zipcode]}" %></td>
      <td><%= "#{DataScrub::capitalize_name(record[:city])}" %></td>
      <td><%= "#{record[:state].upcase}" %></td>
      <td><%= "#{DataScrub::capitalize_name(record[:street])}" %></td>
      <td><%= "#{record[:phone]}" %></td>
      <td><%= "#{record[:district]}" %></td>
    </tr>
    <% end %>
</table>
</body>
</html>
